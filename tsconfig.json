{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "jsx": "preserve",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "CommonJS",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "declaration.d.ts",
    "src/components/panels/Outputs/.tsx"
  ]
}
